{  
    "manifest_version": 2,  
    "name": "mindflow",  
    "description": "This extension is designed for .NET developers",  
    "version": "1.0",  
    "browser_action": {  
        "default_icon": "images/logo.png",  
        "default_popup": "popup.html"  
    },  
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "storage"]
}